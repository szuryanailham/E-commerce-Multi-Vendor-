{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug @e-commerce-multi-vendor/auth-service with Nx",
      "runtimeExecutable": "npx",
      "runtimeArgs": ["nx", "serve", "@e-commerce-multi-vendor/auth-service"],
      "env": {
        "NODE_OPTIONS": "--inspect=9229"
      },
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "skipFiles": ["<node_internals>/**"],
      "sourceMaps": true,
      "outFiles": [
        "${workspaceFolder}/apps/auth-service/dist/**/*.(m|c|)js",
        "!**/node_modules/**"
      ]
    },
